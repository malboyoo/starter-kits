{
  "name": "adonisjs-web-stater-kit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#generated/*": "./.adonisjs/server/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^8.0.0-next.28",
    "@adonisjs/eslint-config": "^3.0.0-next.9",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/tsconfig": "^2.0.0-next.3",
    "@japa/assert": "^4.2.0",
    "@japa/browser-client": "^2.2.0",
    "@japa/plugin-adonisjs": "^5.1.0-next.0",
    "@japa/runner": "^5.0.0",
    "@poppinss/ts-exec": "^1.4.1",
    "@types/alpinejs": "^3.13.11",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.3",
    "alpinejs": "^3.15.3",
    "eslint": "^9.39.2",
    "hot-hook": "^0.4.1-next.1",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "typescript": "~5.9",
    "vite": "^7.3.0",
    "youch": "^4.1.0-beta.13"
  },
  "dependencies": {
    "@adonisjs/auth": "^10.0.0-next.3",
    "@adonisjs/core": "^7.0.0-next.20",
    "@adonisjs/lucid": "^22.0.0-next.4",
    "@adonisjs/session": "^8.0.0-next.2",
    "@adonisjs/shield": "^9.0.0-next.2",
    "@adonisjs/static": "^2.0.0-next.1",
    "@adonisjs/vite": "^5.1.0-next.2",
    "@vinejs/vine": "^4.2.0",
    "better-sqlite3": "^12.5.0",
    "edge.js": "^6.4.0",
    "luxon": "^3.7.2",
    "reflect-metadata": "^0.2.2"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
